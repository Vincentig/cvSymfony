{% extends ":admin:index.html.twig" %}

{% block contenu %}
    {{   parent() }}
    <h1>Experience</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Datedepart</th>
                <td>{{ entity.dateDepart|date('Y-m-d H:i:s') }}</td>
            </tr>
            <tr>
                <th>Datefin</th>
                <td>{{ entity.dateFin|date('Y-m-d H:i:s') }}</td>
            </tr>
            <tr>
                <th>Entreprise</th>
                <td>{{ entity.entreprise }}</td>
            </tr>
            <tr>
                <th>Poste</th>
                <td>{{ entity.poste }}</td>
            </tr>
            <tr>
                <th>Contenu</th>
                <td>{{ entity.contenu }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('admin_experience') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('admin_experience_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
